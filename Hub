---Misc
local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("GUI Toggle")
MiscSection:NewKeybind("Toggle GUI", "Turn Gui on/off", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)

---local Player
local MiscSection = Misc:NewSection("Local Player")

MiscSection:NewSlider("Player Speed", "Changes character speed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MiscSection:NewSlider("Jump Power", "Jump Boost", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

MiscSection:NewButton("Infinite Jump", "Keep on jumping", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Mank-Dude/Infinite-Jump/main/Infintie%20jump'))()
end)

MiscSection:NewButton("Reset WS/JP", "Resets to all defaults", function()
    game.Players.LocalPlayer.character.Humanoid.JumpPower = 50
    game.Players.LocalPlayer.character.Humanoid.WalkSpeed = 16
end)


---Discord
local Discord = Window:NewTab("Discord")
local DiscordSection = Discord:NewSection("Copy to clipboard")

DiscordSection:NewButton("Discord Server", "Want to support or got any questions?", function()
    setclipboard("https://discord.gg/KKFmYKVZ") 
end)

