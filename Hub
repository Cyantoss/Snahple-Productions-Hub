local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Snahple Productions Hub", "BloodTheme")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Credits")
MainSection:NewLabel("HUB made by SNAHPLE PRODUCTIONS")
MainSection:NewLabel("Games Supported:")
MainSection:NewLabel("Blox Fruits")
MainSection:NewLabel("Da Hood")
MainSection:NewLabel("BedWars")
MainSection:NewLabel("NinjaLegends")
MainSection:NewLabel("Combat Warriors")

---Blox Fruits
local BloxFruits = Window:NewTab("BloxFruits")

